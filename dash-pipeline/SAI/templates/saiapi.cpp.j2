    //    goto ErrRet;
    //}
    // TODO: ternaly needs to set priority
    retCode = dashSai->mutateTableEntry(matchActionEntry, p4::v1::Update_Type_INSERT);

    if (grpc::StatusCode::OK == retCode)
    {
        return SAI_STATUS_SUCCESS;
    }
ErrRet:
    return SAI_STATUS_FAILURE;
}

